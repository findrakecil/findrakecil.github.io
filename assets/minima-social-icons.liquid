permalink: /assets/minima-social-icons.svg
---

<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
{% comment %}
  Iterate through {{ site.social_links }} and render platform related SVG-symbol
{% endcomment %}
{% for entry in site.social_links %}
  <symbol id="{{ entry.platform }}" fill-rule="evenodd" clip-rule="evenodd" stroke-linejoin="round" stroke-miterlimit="1.414" viewBox="0 0 16 16">
    {% include social-icons/{{ entry.platform }}.svg %}
  </symbol>
{% endfor %}
</svg>